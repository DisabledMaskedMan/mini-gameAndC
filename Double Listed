struct Node
{
	int val;
	struct Node* next;
	struct Node* prev;
};

struct Node* initial()
{

	struct Node* head = (struct Node*)malloc(sizeof(struct Node));
	head->next = head;
	head->prev = head;
	return head;
}

void backset(struct Node* head, int x)
{
	assert(head);
	struct Node* Nnode = (struct Node*)malloc(sizeof(struct Node));
	Nnode->val = x;
	struct Node* cur = head->prev;
	cur->next = Nnode;
	Nnode->prev = cur;
	Nnode->next = head;
	head->prev = Nnode;
}

void backdel(struct Node* head)
{
	assert(head);
	struct Node* tmp = head->prev;
	head->prev = head->prev->prev;
	head->prev->next = head;
	free(tmp);
	tmp = NULL;
}

void Print(struct Node* head)
{
	struct Node* cur = head->next;
	while (cur != head)
	{
		printf("%d ", cur->val);
		cur = cur->next;
	}
	printf("\n");
}

void text()
{
	struct Node* head = initial();
	backset(head, 1);
	backset(head, 2);
	backset(head, 3);
	backset(head, 4);
	Print(head);
	backdel(head);
	Print(head);
}

int main()
{
	text();
}
