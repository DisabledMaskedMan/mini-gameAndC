#include <stdio.h>

int main()
{
	int a = 1000;
	float b = 1000.0;
	printf("%f\n", (float)a / 11 / 18.8 / 4);
	printf("%f\n", (float)(a / 11) / 18.8 / 4);
	printf("%f\n", (float)(a / 11 / 18.8 / 4));
	printf("%f\n", (float)b / 11 / 18.8 / 4);
	printf("%f\n", b / 11 / 18.8 / 4);
	printf("%f\n", a / 11.0);
	printf("%f\n", (float)(a / 11));
}
